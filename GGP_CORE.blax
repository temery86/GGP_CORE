         WRK43 WRK44 {WRK45} WRK46 WRK47 
         IF WRK47 = yes THEN
            WRK48
         ENDIF   
      ENDIF
      IF BDEMOGRAPHICS.DEM26 = 3 THEN
         WRK49
      ENDIF
   ENDIF
   IF NOT(WRK32 = 1) THEN
      WRK50
   ENDIF
ENDIF  

