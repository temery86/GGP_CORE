   homeowner := no
ENDIF

DEM21
IF DEM21 = yes THEN
   DEM22a DEM22 DEM23 DEM24a
   IF DEM24a = no THEN
      DEM24b | CntFile.lookup.description
      IF CntFile.search(DEM24b) THEN
         DEM24bISO := CntFile.isocode
      ENDIF   
      DEM24e {DEM24c}
   ENDIF
   {DEM24d} DEM25 DEM26 DEM27 DEM28a
   IF DEM28a = yes THEN
      DEM28b
   ENDIF
   IF DEM28a = no THEN
      DEM29a
      IF DEM29a = yes THEN
            DEM29b
      ENDIF
   ENDIF
   DEM30a
   IF DEM30a = yes THEN
      DEM30b
   ENDIF
   IF DEM30a = no THEN
      DEM30c
   ENDIF
   IF (DEM30c = unsure OR DEM30c = probyes OR DEM30c = defyes) AND DEM28a = no THEN
      DEM30d
   ENDIF
ENDIF
IF NOT(DEM28a = yes) AND NOT(DEM29a = yes) THEN
   DEM28c
ENDIF

IF DEM21 = yes THEN   
   IF DEM30a = no THEN
      DEM31 DEM32a
      IF DEM32a = me OR DEM32a = both THEN
         DEM32b
      ENDIF
      IF DEM32a = both OR DEM32a = partner THEN
         DEM32c
      ENDIF
      IF DEM32a = circumstance THEN
         DEM32d
      ENDIF
      IF DEM28a = no THEN
         DEM33
         IF DEM33 = yes THEN
            DEM34
         ENDIF
      ENDIF
      DEM35 DEM36a DEM36au DEM36b DEM36bu
   ENDIF
   DEM37 DEM38 DEM38a DEM38b DEM38c DEM38d DEM38e DEM38f DEM38g
   IF NOT(DEM38a = Never) OR NOT(DEM38b = Never) OR NOT(DEM38c = Never) OR NOT(DEM38d = Never) OR NOT(DEM38e = Never) OR NOT(DEM38f = Never) OR NOT(DEM38g = Never) THEN
      DEM39 DEM39a DEM39b DEM39c DEM39d
   ENDIF
   DEM40 DEM41
   IF DEM41 = yes THEN
      DEM42
   ENDIF   
   DEM43
   IF DEM43 = yes THEN
      DEM44  
   ENDIF
   DEM45
   IF DEM45 = yes THEN
      DEM46 
   ENDIF
ENDIF

