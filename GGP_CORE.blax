   ENDIF
ENDIF


IF ((asex = female AND age < 50) OR (asex = male AND partnerage < 50 AND BDEMOGRAPHICS.DEM23 = female)) AND NOT(FER01a = yes) AND NOT(FER01b = yes) AND hasyoungchild = yes THEN
   FER04b               
